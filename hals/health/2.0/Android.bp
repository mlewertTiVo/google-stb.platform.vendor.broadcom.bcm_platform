cc_binary {
   name: "android.hardware.health@2.0-service.bcm",
   init_rc: ["android.hardware.health@2.0-service.bcm.rc"],
   owner: "broadcom",
   proprietary: true,
   relative_install_path: "hw",
   srcs: [
      "HealthService.cpp",
   ],

   cflags: [
      "-Wall",
      "-Werror",
   ],

   static_libs: [
