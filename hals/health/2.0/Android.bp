      "libbase",
      "libcutils",
      "libhidlbase",
      "libhidltransport",
      "libhwbinder",
      "libutils",
      "android.hardware.health@2.0",
   ],

   header_libs: ["libhealthd_headers"],

   overrides: [
      "healthd",
   ],
}
